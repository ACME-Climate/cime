<?xml version="1.0"?>

<?xml-stylesheet type="text/xsl" href="definitions_variables" ?>

<definitions_variables>

  <entry id="VIC_CONFIG_OPTS">
    <type>char</type>
    <valid_values></valid_values>
    <default_value></default_value>
    <group>build_component_vic</group>
    <desc>VIC configure options</desc>
  </entry> 

  <entry id="VIC_NML_USE_CASE" >
    <type>char</type>
    <valid_values></valid_values>
    <default_value>UNSET</default_value>
    <group>run_component_vic</group>
    <desc>VIC namelist use_case</desc>
  </entry>

  <entry id="VIC_NAMELIST_OPTS">
    <type>char</type>
    <valid_values></valid_values>
    <default_value></default_value>
    <group>run_component_vic</group>
    <desc>VIC specific namelist settings</desc>
  </entry>

  <entry id="VIC_CO2_TYPE"> 
    <type>char</type>
    <valid_values>constant,diagnostic,prognostic</valid_values>
