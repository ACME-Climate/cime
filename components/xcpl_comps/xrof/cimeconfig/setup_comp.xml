<?xml version="1.0"?>

<?xml-stylesheet type="text/xsl" href="setup_comp.xsl" ?>

<setup name="XROF">

  <variable
     name="COMP_ROF" 
     type="char"
     valid_values="xrof"
     value="xrof"  
     group="case_comp"
     sdesc="Name of river component">
  </variable>


  <variable 
     name="XROF_FLOOD_MODE" 
     type="char"
     valid_values="ACTIVE,NULL"
     value="NULL" 
     group="build_component_xrof"
     sdesc="mode for xrof flood feature, NULL means xrof flood is turned off">
  </variable>

</setup>
